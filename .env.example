# Node Environment
NODE_ENV=development
PORT=3000

# Cosmos DB
COSMOS_ENDPOINT=https://your-old-cosmos.documents.azure.com:443/
COSMOS_KEY=your-old-cosmos-key
COSMOS_DATABASE=your-old-database

# New Cosmos DB
NEW_COSMOS_ENDPOINT=https://your-new-cosmos.documents.azure.com:443/
NEW_COSMOS_KEY=your-new-cosmos-key
NEW_COSMOS_DATABASE=your-new-database

# Riot API
RIOT_API_KEY=RGAPI-your-api-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
RIOT_RATE_LIMIT_WINDOW_MS=120000
RIOT_RATE_LIMIT_MAX_REQUESTS=100

# Cache
CACHE_TTL=3600

# Logging
LOG_LEVEL=info

# CORS
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com 